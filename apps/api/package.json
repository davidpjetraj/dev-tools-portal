{
  "name": "@dev-tools-portal/api",
  "version": "1.0.0",
  "private": true,
  "description": "GraphQL API for Dev Tools Portal",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist tsconfig.tsbuildinfo && nest build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "start": "nest start",
